config_version=5

[application]

config/name="RPG-ACTION"
run/main_scene="res://Scenes/TitleScreen/splash_screen_manager.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/image="res://Art/Title_Screen/Punky_Splash.png"
boot_splash/fullsize=false
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://Autoload/Global_Audio_Manager.gd"
LevelManager="*res://Autoload/Globa_Level_Manager.gd"
PlayerHud="*res://Scenes/HUD/Player_Hud.tscn"
PlayerManager="*res://Autoload/Global_Player_Manager.gd"
SceneTransition="*res://Scenes/Over_Lap/scene_transition.tscn"
SaveManager="*res://Autoload/Global_Save_Manager.gd"
PausedMenu="*res://Scenes/HUD/Paused_Menu/Paused_Menu.tscn"
MenuPause="*res://Scenes/HUD/Menu_Pause.tscn"
VariablesWeapons="*res://Autoload/Global_Variables_Weapons.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
